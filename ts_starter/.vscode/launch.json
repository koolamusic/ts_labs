  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "attach",
      "name": "Node Process Typescript",
      "processId": "${command:PickProcess}",
      "address": "localhost",
      "port": 9229,
      "outFiles": [
        
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Typescript Debugger",
      "program": "${workspaceFolder}/app.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}